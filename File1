//Program to find out a person is eligible to vote or not
Console.WriteLine("Enter Age to check eligiblity for Vote:");
int age = int.Parse(Console.ReadLine());
if(age >= 18)
{
    Console.WriteLine(" A person is eligiblity for Voting");
}
else Console.WriteLine("A person is not eligiblity for Voting");
