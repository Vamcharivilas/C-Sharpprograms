// Write a program to check whether the given is /positive or /Negative.
Console.WriteLine("Enter the Number to check Positive or Negative ");
int Num = int.Parse(Console.ReadLine());
if (Num> 0)
{
    Console.WriteLine(Num+"Positive Number");
}
else if (Num == 0)
{
    Console.WriteLine(Num+"ZERO");
}
else Console.WriteLine(Num+"Negative Number");
