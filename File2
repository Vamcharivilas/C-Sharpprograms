//program to print 1-10 Numbers?
Console.WriteLine("using for loop:");
for (int i = 1; i <= 10; i++)
{
    Console.WriteLine(i);
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using while");
int l = 1;
while (l <= 10)
{
    Console.WriteLine(l);
    l++;
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using do-while");
int j = 1;
do
{
    Console.WriteLine(j);
    j++;
}while(j<=10);
_________________________________________________//program to print 10-1 Numbers?
Console.WriteLine("using for loop:");
for (int i = 10; i >= 1; i--)
{
    Console.WriteLine(i);
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using while");
int l = 10;
while (l >= 1)
{
    Console.WriteLine(l);
    l--;
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using do-while");
int j = 10;
do
{
    Console.WriteLine(j);
    j--;
}while(j>=1);
_________________________________________________//program to print 10 to -10 Numbers?
Console.WriteLine("using for loop:");
for (int i = 10; i >= -10; i--)
{
    Console.WriteLine(i);
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using while");
int l = 10;
while (l >= -10)
{
    Console.WriteLine(l);
    l--;
}
Console.WriteLine("+++++++++++++++++");



Console.WriteLine("using do-while");
int j = 10;
do
{
    Console.WriteLine(j);
    j--;
}while(j>=-10);

_________________________________________________//program to print Numbers upto given Number?
Console.Write("Enter any Number:");
int a=int.Parse(Console.ReadLine());   
Console.WriteLine("using for");
for (int i = 1; i <= a; i++)
{
    Console.WriteLine(i);
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using while");
int l = 1;
while (l <= a)
{
    Console.WriteLine(l);
    l++;
}
Console.WriteLine("+++++++++++++++++");
Console.WriteLine("using do-while");
int j = 1;
do
{
    Console.WriteLine(j);
    j++;
} while (j <=a);
