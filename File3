/*program to print MULTIPLULIICATION table of given Number*/
Console.Write("ENTER Any Number To Get Multiplulication of the table:");
int i = int.Parse(Console.ReadLine());
    Console.WriteLine(i + "table:");
    Console.WriteLine("__________");
    for (int j = 1; j <= 10; j++)
        Console.WriteLine(i + "*" + j + " =" + i * j);
    Console.WriteLine();

_________________________________________________________________________________
/*program to print MULTIPLULIICATION table of upto given Number*/
Console.Write("ENTER Any Number To Get Multiplulication of the table:");
int t = int.Parse(Console.ReadLine());
for (int i = 1; i <=t; i++)
{
    Console.WriteLine(i + "table:");
    Console.WriteLine("__________");
    for (int j = 1; j <=10; j++)
        Console.WriteLine(i + "*" + j + " =" + i * j);
    Console.WriteLine();
}
