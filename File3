/*program to print squares of numbers upto given Number*/
Console.Write("ENTER Any Number To Get Squares upto the Number:");
int sq = int.Parse(Console.ReadLine());
for (int i = 0; i <=sq; i++)
Console.WriteLine(i*i);
________________________________________________________________________________
/*program to print Cubes of numbers upto given Number*/
Console.Write("ENTER Any Number To Get Cubes upto the Number:");
int cq = int.Parse(Console.ReadLine());
for (int i = 0; i <=cq; i++)
Console.WriteLine(i*i*i);
